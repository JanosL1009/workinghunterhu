@using MVC_WH.Models
@using MVC_WH.App_Code.Attributes

@model jelentkezes_informatikai_tanfolyamraViewModels
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    string GET_param = Request.QueryString["tanfolyam"];
    htmlJelentkezoLabels ht = new htmlJelentkezoLabels(GET_param);
}

<h1 class="container">Jelentkezés @ht.getPageTitle_H1Tag() tanfolyamra</h1>

<div class="container-fluid WHContent">
    <div class="container">
        <div class="row">

            <div class="col-md-8" style="border-style:solid;">
