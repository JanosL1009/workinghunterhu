                        case "csharp40atipikus":
                            <input type="hidden" id="courseID" name="courseID" value="csharp40atipikus" /> break;
                        case "csharp80atipikus":
                            <input type="hidden" id="courseID" name="courseID" value="csharp80atipikus" /> break;
                        case "aspdotnet80atipikus":
                            <input type="hidden" id="courseID" name="courseID" value="aspdotnet80atipikus" /> break;
                        case "php80atipikus":
                            <input type="hidden" id="courseID" name="courseID" value="php80atipikus" /> break;
                        case "linuxtanfolyam":
                            <input type="hidden" id="courseID" name="courseID" value="linuxtanfolyam" /> break;
                        case "pythontanfolyam":
                            <input type="hidden" id="courseID" name="courseID" value="pythontanfolyam" /> break;
                        default:
                            <input type="hidden" id="courseID" name="courseID" value="no course" />
                            break;
                    }




