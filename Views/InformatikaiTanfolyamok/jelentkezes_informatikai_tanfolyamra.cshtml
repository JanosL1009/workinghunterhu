                            <td>
                                <input id="UjSzamlazasiCim" type="checkbox" name="UjSzamlazasiCim" class="" />
                            </td>
                        </tr>

                        <tr class="hideTR">
                            <td><b>Számlázási név:</b></td>
                            <td>
                                <input name="SzamlazasiNev" type="text" id="SzamlazasiNev" class="form-control" />
                            </td>
                        </tr>

                        <tr class="hideTR">
                            <td><b>Számlázási cím:</b></td>
                            <td>
                                <input name="SzamlazasiCim" type="text" id="SzamlazasiCim" class="form-control" />
                            </td>
                        </tr>

                        <tr class="hideTR">
                            <td><b>Adószám:(vállalkozóknak/vállalkozásoknak)</b></td>
                            <td>
                                <input name="TaxNumber" type="text" id="TaxNumber" class="form-control" />
                            </td>
                        </tr>

                        <tr>
                            <td><b>Megjegyzés:</b></td>
                            <td>
                                <textarea name="Megjegyzes" rows="10" cols="35" id="Megjegyzes"></textarea>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2">
                                @Html.CheckBoxFor(m => Model.Gdpr)
                                <a href="http://workinghunter.com/infotanf-adatvedelmi-tajekoztato.aspx" target="_blank">Itt tekintheted meg az Adatévelmi tájékoztatót!</a>
                                <label>Kérjük, hogy figyelmesen olvasd el az Adatvédelmi tájékoztatónkat. A tájékoztatóból megtudhatod, hogy hogyan kezeljük és védjük adataidat!A mező elfogadásával megerősíted,  hogy elolvastad és elfogadod azt! Hozzájárulsz személyes adataid kezeléséhez!</label>
                            </td>

                            <span id="custCheck" style="visibility:hidden;">Check CheckBox</span>

                        </tr>

                        <tr>
                            <td colspan="2">
                                @Html.CheckBoxFor(m => Model.TermsAndConditions)
                                @Html.ValidationMessageFor(m => Model.TermsAndConditions)
                                <a href="http://workinghunter.com/infotanf-altalanos-szerzodesi-feltetelek.aspx" target="_blank">Itt tekintheted meg az Általános Szerződési Feltételeket!</a>
                                <label>Kérjük, hogy figyelmesen olvasd el az ÁSZF-et. A ÁSZF-ből megtudhatod a Szoláltatással kapcsolatos összes részletet!A mező elfogadásával megerősíted,  hogy elolvastad és elfogadod azt!</label>


                            </td>

                            <span id="CustomValidator2" style="visibility:hidden;">Check CheckBox</span>

                        </tr>

                        <tr>
                            <td></td>
                            <td>
                                <input type="submit" name="Submit" value="Jelentkezés" class="form-control" />
                            </td>

                        </tr>



                    </table>
                }

            </div>

            <div class="col-md-4" style="border-style:dotted;">


            </div>
        </div>

    </div>

</div>
<script>

    $(document).ready(function () {

        ($("input#UjSzamlazasiCim").change(function () {
            if ($("input#UjSzamlazasiCim").prop("checked")) {
                $(".hideTR").show();
            } else { $(".hideTR").hide(); }
        }));



    });
</script>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
